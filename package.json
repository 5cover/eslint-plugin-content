{
  "name": "@5cover/eslint-plugin-content",
  "version": "1.0.5",
  "private": false,
  "description": "ESLint plugins to enforce the content of your code rather than form",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "homepage": "https://github.com/5cover/eslint-plugin-content",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/5cover/eslint-plugin-content.git"
  },
  "license": "MIT",
  "author": "5cover",
  "type": "module",
  "exports": "./dist/lib/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:ts": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "mocha",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.17",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-doc-generator": "^2.0.0",
    "eslint-plugin-eslint-plugin": "^6.0.0",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "jiti": "^2.4.2",
    "mocha": "^11.0.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "url": "^0.11.4"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  }
}
